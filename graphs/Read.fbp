'.+' -> REGEXP FilterToken(groups/FilterByGroup)
'true' -> RECURSE FilterAttributes(objects/FilterProperty)
'^_.+' -> WITH SendFilter(packets/SendWith) OUT -> KEY FilterAttributes()
FilterToken() OUT -> IN Construct(pgorm/ConstructRead)

FilterToken() GROUP -> TOKEN ReadServer(pg/Postgres)
Construct() TEMPLATE -> TEMPLATE ReadServer()
Construct() OUT -> IN ReadServer()

# Convert to NoFlo packets

'out' -> KEY ExtractRow(objects/ExtractProperty)
'_type' -> PROPERTY GroupByType(underscore/GroupBy)
ReadServer() OUT -> IN ExtractRow() OUT -> IN ParseRows(strings/ParseJson) OUT -> IN GroupByType() OUT -> IN FilterAttributes() OUT -> IN Packetize(adapters/ObjectToPackets)
